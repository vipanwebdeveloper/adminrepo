                function parseDate(s) {
                    var dateTime = s.split(' ');
                    var dateBits = dateTime[0].split('-');
                    var timeBits = dateTime[1].split(':');
                    return new Date(dateBits[0], parseInt(dateBits[1]) - 1, dateBits[2], timeBits[0], timeBits[1], timeBits[2]).valueOf();
                }
                function dateFilter2(dateTime) {
                    returnFormat='MMM D, YYYY h:mma';
                    {%if (app.user.timeZone!=null) %}
                        {%set defaultTimeZone=app.user.timeZone.zoneName%}
                    {%else%}
                        {%set defaultTimeZone='UTC'%}
                    {%endif%}


                    var newYork = moment.tz(dateTime, "UTC");
                    var losAngeles = newYork.clone().tz("{{defaultTimeZone}}");
                    
                    return losAngeles.format(returnFormat);
                }
                
