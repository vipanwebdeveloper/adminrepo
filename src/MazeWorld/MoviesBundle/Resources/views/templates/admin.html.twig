            <script>
                $("#container").hide();

                function ProgBar(){
                    this.bar = null;
                    this.setBar = function(){
                        $("#container").show();

                        this.bar = new ProgressBar.Circle('#container', {
                            color: '#FFEA82',
                            trailColor: '#eee',
                            trailWidth: 1,
                            duration: 1400,
                            easing: 'easeIn',
                            strokeWidth: 15,
                            from: {color: '#3881f7', a: 0},
                            to: {color: '#f73838', a: 1},
                            // Set default step function for all animate calls
                            step: function (state, circle) {
                                circle.path.setAttribute('stroke', state.color);
                            }
                        });
                    };

                    this.setVal = function(value, duration){
                        this.bar.animate(value, {
                            duration: duration
                        });
                        return true;
                    };

                    this.killBar = function(bar, delay){
                        setTimeout(function(){
                            bar.stop();
                            $("#container").empty();
                            $("#container").hide();
                        }, delay);
                    };
                }

                function startSpinner()
                {
                    $("#container").addClass('heartbeat-loader');
                    $(".wrapper").css("opacity",".3");
                    $("#container").show();
                }
                function endSpinner()
                {
                    $("#container").removeClass('heartbeat-loader');
                    $(".wrapper").css("opacity","1");
                    $("#container").hide();
                }

                //window.onerror = function(message, url, lineNumber) {return true;};
                
                $("form.navbar-form").bind("keydown", function(e) {
				   if (e.keyCode == 13) return false;
				 });
                
            </script>
