                {%if app.user.timeZone %}var todayDate = moment().tz("{{app.user.timeZone.zoneName}}").format();
				var defaultTimeZone = "{{app.user.timeZone.zoneName}}";
                {%else%}
                    var todayDate = moment().tz("UTC").format();
                    var defaultTimeZone = "UTC";
                {%endif%}
                
                function mtz(m) {
					{%if app.user.timeZone %}return moment.tz(m, "{{app.user.timeZone.zoneName}}");
					{%else%}return moment.tz(m, "UTC");
					{%endif%}}
                

                function dateFilter(dateTime) {
                
                    returnFormat='MMM Do h:mma';
                    {%if (app.user.timeZone!=null) %}
                        {%set defaultTimeZone=app.user.timeZone.zoneName%}
                    {%else%}
                        {%set defaultTimeZone='UTC'%}
                    {%endif%}


                    var newYork = moment.tz(dateTime, "UTC");
                    var losAngeles = newYork.clone().tz("{{defaultTimeZone}}");
                    
                    return losAngeles.format(returnFormat);
                }

